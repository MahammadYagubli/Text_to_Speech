import pyttsx3
engine=pyttsx3.init()
engine.say('Helolo world')
engine.setProperty('gender', 'female')
engine.setProperty('rate', 160)
engine.setProperty('volume', 0.9)
engine.runAndWait()

engine.setProperty('voice', 'azery')
engine.setProperty('gender', 'female')
engine.say('Səni çox sevirəm')
engine.runAndWait()
